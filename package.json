{
	"name": "companion",
	"version": "3.0.0",
	"description": "Companion",
	"main": "main.js",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "yarn dev:inner --log-level debug",
		"dev:inner": "yarn build:writefile && nodemon --ignore './webui/' --ignore './launcher/' --ignore './dist/' -- main.js --admin-address 127.0.0.1 --extra-module-path=./module-local-dev",
		"dev:debug": "yarn dev:inner --log-level silly",
		"update": "./tools/update.sh",
		"dist:webui": "yarn --cwd webui build",
		"dist": "zx ./tools/build/complete.mjs",
		"macdist": "yarn dist mac-x64",
		"macarmdist": "yarn dist mac-arm64",
		"windist": "yarn dist win-x64",
		"lindist": "yarn dist linux-x64",
		"rpidist": "yarn dist linux-arm7",
		"dist:prepare": "zx tools/build_prepare.mjs",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"dev-webui": "yarn --cwd webui dev",
		"format": "prettier --write .",
		"jsdoc": "jsdoc -c jsdoc.json",
		"build:writefile": "zx ./tools/build_writefile.mjs",
		"module:bulk": "zx ./tools/module_bulk.mjs"
	},
	"repository": "https://github.com/bitfocus/companion",
	"keywords": [
		"bitfocus",
		"companion"
	],
	"engines": {
		"npm": "please-use-yarn",
		"yarn": "^1.22",
		"node": ">=18.12 <19"
	},
	"author": "Bitfocus AS",
	"license": "MIT",
	"devDependencies": {
		"@sentry/webpack-plugin": "^1.20.0",
		"jest": "^29.3.1",
		"jsdoc": "github:bitfocus/jsdoc#3.6.6-patch",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"tar": "^6.1.11",
		"tui-jsdoc-template": "^1.2.2",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"zx": "^7.1.1"
	},
	"dependencies": {
		"@companion-module/base": "^1.0.0",
		"@elgato-stream-deck/node": "^5.7.0",
		"@jsep-plugin/numbers": "^1.0.1",
		"@jsep-plugin/template": "^1.0.2",
		"@julusian/jpeg-turbo": "^2.0.0",
		"@loupedeck/node": "^0.3.1",
		"@sentry/integrations": "^7.25.0",
		"@sentry/node": "^7.19.0",
		"@sentry/tracing": "^7.19.0",
		"archiver": "^5.3.1",
		"body-parser": "^1.20.1",
		"bufferutil": "^4.0.7",
		"commander": "^9.4.1",
		"debounce-fn": "^5.1.2",
		"ejson": "^2.2.3",
		"emberplus-connection": "^0.1.2",
		"env-paths": "^3.0.0",
		"express": "^4.18.2",
		"express-serve-zip": "^1.0.1",
		"fast-json-patch": "^3.1.1",
		"find-process": "1.4.7",
		"fs-extra": "^10.1.0",
		"get-port": "^6.1.2",
		"got": "^12.5.2",
		"infinitton-idisplay": "^1.1.2",
		"jsep": "^1.3.8",
		"jsonpath-plus": "^7.2.0",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.4",
		"nanoid": "^4.0.0",
		"node-machine-id": "^1.1.12",
		"osc": "^2.4.3",
		"p-debounce": "^4.0.0",
		"p-queue": "^7.3.0",
		"path-to-regexp": "^6.2.1",
		"pngjs": "^3.3.3",
		"respawn": "link:./vendor/respawn",
		"selfsigned": "^2.1.1",
		"semver": "^7.3.8",
		"sharp": "^0.31.2",
		"socket.io": "^4.5.3",
		"socketcluster-client": "^16.1.1",
		"supports-color": "^9.2.3",
		"usb": "^2.5.2",
		"utf-8-validate": "^5.0.10",
		"winston": "^3.8.2",
		"ws": "^8.11.0",
		"xkeys": "^2.4.0"
	},
	"collective": {
		"type": "donorbox",
		"url": "https://donorbox.org/bitfocus-opensource"
	},
	"resolutions": {
		"node-gyp-build": "github:julusian/node-gyp-build#cross-install-support"
	}
}
