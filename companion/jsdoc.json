{
	"source": {
		"include": ["lib", "package.json", "../README.md"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs/)"
	},
	"opts": {
		"encoding": "utf8",
		"readme": "../README.md",
		"destination": "docs/jsdoc/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"name": "Companion",
		"footerText": " ",
		"logo": {
			"url": "https://bitfocus.io/images/topbar-logowhite.png",
			"width": "147px",
			"height": "24px",
			"link": "https://bitfocus.io"
		},
		"useCollapsibles": true
	}
}
